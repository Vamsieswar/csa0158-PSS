#include<stdio.h>
int main()
{
	int n;
	printf("enter score:");
	scanf("%d",&n);
	switch(n/10)
	{
		case 10:
		 printf("grade s");
		 break;
		case 9:
		 printf("grade a");
		 break;
	    case 8:
	     printf("grade b");
	     break;
	    case 7:
	     printf("grade c");
	     break;
	    case 6:
	     printf("grade d");
	     break;
	    case 5:
	     printf("grade e");
	     break;
	    default:
	    	printf("grade f");
	    	break;
	}
	return 0;	
}
